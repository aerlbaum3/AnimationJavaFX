<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="rootVBox" xmlns="http://javafx.com/javafx" 
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="application.StackController"> 
	
    <HBox spacing="10">
        <TextField fx:id="inputField" promptText="Enter integer"/>
        <Button text="Push" onAction="#pushButtonClicked"/>
        <Button text="Pop" onAction="#popButtonClicked"/>
        
    </HBox>
</VBox>
